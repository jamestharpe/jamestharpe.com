		"Whitelabel",
		"allowfullscreen",
		"assertj",
		"autoconfigure",
		"autoplay",
		"buildscript",
		"calintat",
		"classpath",
		"esversion",
		"fasterxml",
		"focusring",
		"gradlew",
		"innodb",
		"intellij",
		"jayway",
		"jdbc",
		"jsonpath",
		"necolas",
		"prettierrc",
		"prismjs",
		"solarizedlight",
		"springframework",
		"submodules",
		"superelse",
		"superif",
		"textfield",
		"trackerapi",
		"uroot",
		"xargs"
