{
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "node_modules": true
	},
  "cSpell.words": [
		"Autowired",
		"Berardi",
		"Cblue",
		"Creset",
		"Docker's",
		"Dockerfile's",
		"Dockerize",
		"ENTRYPOINT",
