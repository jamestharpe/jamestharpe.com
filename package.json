	"bugs": {
		"url": "https://github.com/jamestharpe/jamestharpe.com/issues"
	},
	"dependencies": {
		"@mdx-js/mdx": "^1.6.22",
		"@mdx-js/react": "^1.6.22",
		"@xstate/react": "^1.6.1",
		"gatsby": "^3.10.1",
		"gatsby-image": "^3.11.0",
		"gatsby-plugin-feed-mdx": "^1.0.1",
		"gatsby-plugin-lunr": "^1.5.2",
		"gatsby-plugin-manifest": "^3.14.0",
		"gatsby-plugin-mdx": "^2.6.0",
		"gatsby-plugin-meta-redirect": "^1.1.1",
		"gatsby-plugin-offline": "^4.14.0",
		"gatsby-plugin-react-helmet": "^4.14.0",
		"gatsby-plugin-sharp": "^3.14.0",
		"gatsby-redirect-from": "^0.3.0",
		"gatsby-remark-katex": "^4.2.0",
		"gatsby-source-filesystem": "^3.2.0",
		"gatsby-transformer-sharp": "^3.2.0",
		"katex": "^0.13.0",
		"nstall": "^0.2.0",
		"prop-types": "^15.7.2",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-helmet": "^6.1.0",
		"remark-html-katex": "^3.0.0",
		"remark-math": "^3.0.1",
		"vis-data": "^7.1.1",
		"vis-network": "^8.5.5",
		"xstate": "^4.20.0"
	},
	"devDependencies": {
		"@tmus/gatsby-remark-links-normalizer": "git+https://github.com/tmobile/gatsby-remark-links-normalizer.git",
		"@types/lunr": "^2.3.3",
		"@types/react-helmet": "^6.1.0",
		"@typescript-eslint/eslint-plugin": "^4.7.0",
		"@typescript-eslint/parser": "^4.7.0",
		"eslint": "^7.13.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.21.5",
		"gatsby-plugin-catch-links": "^3.14.0",
		"gatsby-plugin-feed": "^3.14.0",
		"gatsby-plugin-google-gtag": "^3.14.0",
		"gatsby-remark-graph": "^0.2.4",
		"gatsby-remark-prismjs": "^4.2.0",
		"husky": "^7.0.0",
		"markdown-link-check": "^3.8.5",
		"markdownlint-cli": "^0.22.0",
		"prettier": "2.1.2",
		"prismjs": "^1.24.1",
		"ts-node": "^9.0.0",
		"typescript": "^4.0.5"
	},
	"keywords": [
		"gatsby",
		"website",
		"james-tharpe"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/jamestharpe/jamestharpe-website"
	},
	"scripts": {
		"build": "gatsby build",
		"build:clean": "npm run clean && npm run build",
		"clean": "gatsby clean",
		"deploy": "./deploy.sh",
		"lint:fix": "npm run lint:react:fix && npm run lint:markdown:fix",
		"lint:markdown": "markdownlint '**/*.md'",
		"lint:markdown:fix": "markdownlint '**/*.md' --fix",
		"lint:react:fix": "eslint . --fix",
		"lint:react": "eslint .",
		"lint": "npm run lint:react && npm run lint:markdown",
		"serve": "gatsby serve",
		"start:clean": "npm run clean && gatsby develop",
		"start": "gatsby develop",
		"test:links": "for file in $(find . -path ./node_modules -prune -false -o -name \"*.md\"); do markdown-link-check --verbose --config .markdown-link-check.config.json \"$file\" || exit 1; done;",
		"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
		"prepare": "husky install"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": "eslint --cache --fix",
		"*.{md,mdx}": "markdownlint --fix"
	}
}
