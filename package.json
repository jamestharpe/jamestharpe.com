{
	"author": "James Tharpe",
	"description": "James Tharpe's Website",
	"license": "MIT",
	"name": "jamestharpe.com",
	"private": true,
	"version": "0.1.0",
	"bugs": {
		"url": "https://github.com/jamestharpe/jamestharpe.com/issues"
	},
	"dependencies": {
		"gatsby": "^2.24.85",
		"gatsby-image": "^2.4.19",
		"gatsby-plugin-lunr": "^1.5.2",
		"gatsby-plugin-manifest": "^2.4.30",
		"gatsby-plugin-meta-redirect": "^1.1.1",
		"gatsby-plugin-offline": "^3.2.28",
		"gatsby-plugin-react-helmet": "^3.3.11",
		"gatsby-plugin-sharp": "^2.6.36",
		"gatsby-redirect-from": "^0.2.4",
		"gatsby-source-filesystem": "^2.3.35",
		"gatsby-transformer-remark": "^2.8.46",
		"gatsby-transformer-sharp": "^2.5.15",
		"prop-types": "^15.7.2",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-helmet": "^6.1.0",
		"vis-data": "^7.1.1",
		"vis-network": "^8.5.5"
	},
	"devDependencies": {
		"@tmus/gatsby-remark-links-normalizer": "git+https://github.com/tmobile/gatsby-remark-links-normalizer.git",
		"@types/lunr": "^2.3.3",
		"@types/react-helmet": "^6.1.0",
		"@typescript-eslint/eslint-plugin": "^4.7.0",
		"@typescript-eslint/parser": "^4.7.0",
		"eslint": "^7.13.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.21.5",
		"gatsby-plugin-catch-links": "^2.5.0",
		"gatsby-plugin-feed": "^2.8.0",
		"gatsby-plugin-google-gtag": "^2.3.0",
		"gatsby-remark-graph": "^0.2.3",
		"gatsby-remark-katex": "^3.5.0",
		"gatsby-remark-prismjs": "^3.7.0",
		"katex": "^0.12.0",
		"markdown-link-check": "^3.8.5",
		"markdownlint-cli": "^0.22.0",
		"prettier": "2.1.2",
		"prismjs": "^1.22.0",
		"ts-node": "^9.0.0",
		"typescript": "^4.0.5"
	},
	"keywords": [
		"gatsby",
		"website",
		"james-tharpe"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/jamestharpe/jamestharpe-website"
	},
	"scripts": {
		"build": "gatsby build",
		"build:clean": "npm run clean && npm run build",
		"clean": "gatsby clean",
		"deploy": "./deploy.sh",
		"lint:fix": "npm run lint:react:fix && npm run lint:markdown:fix",
		"lint:markdown": "markdownlint '**/*.md' --ignore node_modules",
		"lint:markdown:fix": "markdownlint '**/*.md' --fix --ignore node_modules",
		"lint:react:fix": "eslint . --fix",
		"lint:react": "eslint .",
		"lint": "npm run lint:react && npm run lint:markdown",
		"serve": "gatsby serve",
		"start:clean": "npm run clean && gatsby develop",
		"start": "gatsby develop",
		"test:links": "for file in $(find . -path ./node_modules -prune -false -o -name \"*.md\"); do markdown-link-check --verbose --config .markdown-link-check.config.json \"$file\" || exit 1; done;",
		"test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
	}
}
